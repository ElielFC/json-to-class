{
    "name": "elielfc/to-class",
    "description": "Converta seu json ou array em class personalizadas.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eliel F Canivarolli",
            "email": "eliel.f.canivarolli@gmail.com"
        }
    ],
    "require": {
		"php": "^8.0"
    },
    "require-dev": {
		"phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "ElielFC\\ToClass\\": "src/"
        }
    },
    "autoload-dev": {
       "psr-4": {
           "ElielFC\\ToClass\\Test\\": "tests/"
       }
   },
   "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-clover codecov.xml"
   }
}
